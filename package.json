{
  "name": "littlelightshow",
  "version": "1.0.0",
  "type": "module",
  "description": "littlelightshow",
  "main": "index.js",
  "scripts": {
    "debug": "netlify dev --inspect",
    "build:admin": "esbuild --watch --format=esm --bundle app/admin.ts --outfile=bin/admin.js",
    "build:test": "esbuild --watch --format=esm --bundle test/test.ts --outfile=bin/test.js",
    "build:invoice": "esbuild --watch --format=esm --bundle app/invoice/invoice.ts --outfile=bin/invoice.js",
    "build:gl": "esbuild --watch --format=esm --bundle app/gl/gl.ts --outfile=bin/gl.js",
    "build": "npm-run-all -p build:gl build:invoice"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corey-alix/littlelightshow.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/corey-alix/littlelightshow/issues"
  },
  "homepage": "https://github.com/corey-alix/littlelightshow#readme",
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@types/node": "^14.18.0",
    "esbuild": "^0.14.3",
    "faunadb": "^4.4.1",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "netlify-lambda": "^2.0.15",
    "npm-run-all": "^4.1.5"
  }
}
